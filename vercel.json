{
  "version": 2,
  "builds": [
    {
      "src": "./app.py",
      "use": "@vercel/python"
    }
  ],
  
  "functions": {
    "app.py": {
      "memory": 3009,
      "maxDuration": 58
    },
    "*.py": {
      "memory": 3009,
      "maxDuration": 58
    }
  },

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
